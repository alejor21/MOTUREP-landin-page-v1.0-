# Optimizaciones para bajo uso de memoria
# Estas variables reducen el consumo de recursos durante el desarrollo

# Desactiva source maps en desarrollo para ahorrar memoria
GENERATE_SOURCEMAP=false

# Reduce el uso de memoria de Node.js
NODE_OPTIONS=--max-old-space-size=2048

# Desactiva algunas caracter√≠sticas de desarrollo para mejorar rendimiento
FAST_REFRESH=true
DISABLE_ESLINT_PLUGIN=true
